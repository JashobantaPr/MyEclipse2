package practice;

public class PrimeNumberCheck {
public static void main(String[] args) {
	int n=7;
	int sum=0;
	for (int i = 1; i <=n; i++) {
		if (n%i==0) {
			sum+=i;
		}
	}
	if (sum==2) {
		System.out.println(n+" is a prime number");
	}else {
		System.out.println(n+" is not a prime number");
	}
}
}
